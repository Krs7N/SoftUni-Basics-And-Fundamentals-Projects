<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
	<CodeSnippet Format="1.0.0">
		<Header>
			<Title>intArray</Title>
			<Shortcut>intArray</Shortcut>
			<Description>Code snippet for int Array</Description>
			<Author>Kristian Mladenov</Author>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
				<SnippetType>SurroundsWith</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>varName</ID>
					<Default>arr</Default>
					<ToolTip>variable name.</ToolTip>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[int[] $varName$ = Console.ReadLine().Split().Select(int.Parse).ToArray();]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>